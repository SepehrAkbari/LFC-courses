    "def recursive_arithmetic_sequence(a, d, n):\n",
    "    if n == 1:\n",
    "        return a + d\n",
    "    else:\n",
    "        return d + recursive_arithmetic_sequence(a, d, n-1)\n",
    "\n",
    "\n",
    "print(recursive_arithmetic_sequence(1, 3, 4))"
   ]
